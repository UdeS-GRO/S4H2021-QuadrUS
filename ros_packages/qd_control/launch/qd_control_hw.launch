<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find qd_control)/config/qd_control_test.yaml" command="load"/>

  <node name="QuadrusHardwareInterface" pkg="qd_control" type="qd_hw_interface" output="screen"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="
      /quadrus/joint1_position_controller
      /quadrus/joint_state_controller"/>

</launch>
